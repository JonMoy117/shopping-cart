


{{!-- <h1>Dynamic Items</h1>
<h6>There are {{product.length}} posts</h6> --}}


{{#each product}}
<div class="row">
    <h1>Category: {{description}} </h1>
    <form action="/shopping/{{id}}/add_to_cart" method='post'>
        <input type="submit" class='btn btn-primary'>
    </form>
    <div class="row1">
        <div class="col-sm-3"><h2>{{title}}</h2></div>
        <div class="col-sm-3"><h3>${{price}}</h3></div>
        <div class="col-sm-6"><img src= '/uploads/{{imagePath}}'/></div>
    </div>
<form action="/shopping/{{id}}/product_showpage">
        <input type="submit" class='btn btn-primary' method='get'>
        </form>
</div>
{{/each}}

